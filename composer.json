{
    "name": "tgerbet/babelfish",
    "description": "A file programming language detector",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Thomas Gerbet",
            "email": "thomas@gerbet.me"
        }
    ],
    "require": {
        "php": "^7.2"
    },
    "require-dev": {
        "symfony/yaml": "^4.1",
        "doctrine/coding-standard": "^6.0",
        "vimeo/psalm": "^3.0",
        "phpunit/phpunit": "^8.0"
    },
    "autoload": {
        "psr-4": {
            "Babelfish\\": "src/Babelfish"
        },
        "exclude-from-classmap": ["src/Babelfish/Internal/"]
    },
    "autoload-dev": {
        "psr-4": {
            "BabelfishTest\\": "tests/BabelfishTest"
        }
    }
}
